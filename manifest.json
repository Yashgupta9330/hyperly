{
    "manifest_version": 3,
    "name": "CRXJS React Vite Example with LinkedIn Auth",
    "version": "1.0.0",
    "permissions": [
      "identity",
      "storage"
    ],
    "host_permissions": [
      "https://www.linkedin.com/*"
    ],
    "content_scripts": [
      {
        "js": ["src/content.tsx"],
        "matches": ["https://www.linkedin.com/*"]
      }
    ],
    "action": {
      "default_popup": "index.html"
    }
  }
  